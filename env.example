# Telegram API credentials (получить на https://my.telegram.org/apps)
TELEGRAM_API_ID=your_api_id_here
TELEGRAM_API_HASH=your_api_hash_here

# Telegram channel to parse (без @)
TELEGRAM_CHANNEL_USERNAME=durov

# Количество последних постов для парсинга
POSTS_LIMIT=100

# MongoDB connection
MONGODB_URI=mongodb://localhost:27017/
MONGODB_DB_NAME=events_db

# Имя сессии Telethon (будет создан файл .session)
TELEGRAM_SESSION_NAME=telegram_parser_session

# Директория для сохранения скачанных фото (опционально)
PHOTOS_DIR=downloaded_photos

